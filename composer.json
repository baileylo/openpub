{
    "name": "baileylo/blog",
    "description": "Simple blogging platform.",
    "keywords": [
        "blog"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/baileylo/Commander.git"
        }
    ],
    "require": {
        "laravel/framework": "4.2.*",
        "doctrine/mongodb-odm": "~1.0.0-BETA11@dev",
        "ext-yaml": "~1.1",
        "league/commonmark": "~0.6@dev",
        "phpunit/phpunit": "~4.",
        "laracasts/commander": "dev-commander-events",
        "league/tactician": "dev-master",
        "nickcernis/html-to-markdown": "dev-master",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "classmap": [
            "app/tests/TestCase.php"
        ],
        "psr-4": {
            "Baileylo\\BlogApp\\": "app",
            "Baileylo\\Blog\\": "Blog",
            "Baileylo\\Core\\": "Core"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
